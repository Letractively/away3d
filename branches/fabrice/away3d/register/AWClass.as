package away3d.register {//import flash.display.Sprite;import away3d.register.AWClassManager;//	public class AWClass extends Sprite {		public var strName:String = "";		/**		*	This method Registers a class with an id. This class is a work arround for singleton construction		*/		function AWClass() {}		public function register(strName:String = ""):void{			this.strName = strName;			//trace("register "+this.strName);			AWClassManager.register(this, this.strName);		}	}}