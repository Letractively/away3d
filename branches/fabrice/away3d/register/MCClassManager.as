package away3d.register{	public class MCClassManager {		public static var instance:MCClassManager = new MCClassManager();		private var oClass:Object = new Object();		function MCClassManager() {		}		public static function register(instanceSprite:Object = null, strName:String = ""):void {			 			var key:String = instanceSprite.strName;			if (key == "" || !instanceSprite) {				throw new Error("class is undefined");			}			if(getMCClass(key) != null){				trace("register overwrite on '"+key+"'");			}						MCClassManager.instance.oClass[key] = instanceSprite;			//trace("--> registered "+strName);		}		public static function getMCClass(key:String = ""):Object {			if (MCClassManager.instance.oClass[key] == "") {				throw new Error(key + " MCClass is undefined");			}			return MCClassManager.instance.oClass[key];		}	}}