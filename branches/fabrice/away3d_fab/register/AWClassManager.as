package away3d.register{	public class AWClassManager {		public static var instance:AWClassManager = new AWClassManager();		private var oClass:Object = new Object();		function AWClassManager() {		}		public static function register(instanceSprite:Object = null, strName:String = ""):void {			 			var key:String = instanceSprite.strName;			if (key == "" || !instanceSprite) {				throw new Error("class is undefined");			}			if(getClass(key) != null){				trace("register overwrite on '"+key+"'");			}						AWClassManager.instance.oClass[key] = instanceSprite;			//trace("--> registered "+strName);		}		public static function getClass(key:String = ""):Object {			if (AWClassManager.instance.oClass[key] == "") {				throw new Error(key + " MCClass is undefined");			}			return AWClassManager.instance.oClass[key];		}	}}