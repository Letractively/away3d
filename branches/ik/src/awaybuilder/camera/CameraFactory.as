package awaybuilder.camera{	import awaybuilder.vo.SceneCameraVO;				/**	 * @author andreasengstrom	 */	public class CameraFactory	{		private var propertyFactory : CameraPropertyFactory ;								public function CameraFactory ( )		{			this.initialize ( ) ;		}								////////////////////		// PUBLIC METHODS //		////////////////////								public function build ( vo : SceneCameraVO ) : SceneCameraVO		{			vo = this.propertyFactory.build ( vo ) ;			return vo ;		}								public function toString ( ) : String		{			return "CameraFactory" ;		}								/////////////////////		// PRIVATE METHODS //		/////////////////////								private function initialize ( ) : void		{			this.propertyFactory = new CameraPropertyFactory ( ) ;		}	}}