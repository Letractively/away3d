package away3d.animators.data{	import away3d.core.math.Number3D;	/**	 * Holds information about a segment of a curve.	 */    public class CurveSegment    {    	/**    	 * coordinates first point.    	 */        public var v0:Number3D;				/**    	 * coordinates anchor for this curve.    	 */        public var va:Number3D;				/**    	 * coordinates end point.    	 */        public var v1:Number3D;    			/**		 * Creates a new <code>Segment</code> object.		 * 		 * @param	 v0			A Number3D		 * @param	 va			A Number3D		 * @param	 v1			A Number3D		 */        public function CurveSegment(v0:Number3D, va:Number3D, v1:Number3D)        {            this.v0 = v0;			this.va = va;			this.v1 = v1;        }		    }}