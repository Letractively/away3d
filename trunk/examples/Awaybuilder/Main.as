package{	import awaybuilder.WorldBuilder;	import awaybuilder.collada.ColladaLoader;		import flash.display.Sprite;	import flash.events.Event;	import flash.net.URLRequest;				public class Main extends Sprite	{		public function Main ( )		{			var loader : ColladaLoader = new ColladaLoader ( ) ;						loader.addEventListener ( Event.COMPLETE , this.loadComplete ) ;			loader.load ( new URLRequest ( "world.dae" ) ) ;		}								private function loadComplete ( event : Event ) : void		{			var loader : ColladaLoader = event.target as ColladaLoader ;			var world : WorldBuilder = new WorldBuilder ( ) ;						world.data = loader.collada ;			world.startCamera = "overviewCam" ;			this.addChild ( world ) ;			world.x = Math.floor ( this.stage.stageWidth * 0.5 ) ;			world.y = Math.floor ( this.stage.stageHeight * 0.5 ) ;			world.build ( ) ;		}	}}